{
  "name": "terminal-tracker",
  "displayName": "Terminal Command Tracker",
  "description": "Tracks VSCode terminal commands and displays usage statistics in a GitHub-style heatmap",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:terminal-tracker.showHeatmap",
    "onCommand:terminal-tracker.logCommand"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminal-tracker.showHeatmap",
        "title": "Terminal Tracker: Show Command Heatmap"
      },
      {
        "command": "terminal-tracker.logCommand",
        "title": "Terminal Tracker: Log Command Manually"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "echo 'Skipping tests'"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "glob": "^7.1.6",
    "typescript": "^4.2.4"
  }
}